{
	"compilerOptions": {
		"outDir": "./build",
		"allowJs": true,
		"target": "es5",
		"jsx": "react",
		"noEmitOnError": true,
		"noUnusedLocals": true,
		"removeComments": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"baseUrl": "./",
	},
	"include": ["./src", "declaration.d.ts"],
	"exclude": ["node_modules", "./build/*", "./dist/*"],
}
